<!-- Plugin Configuration File. Read more: https://plugins.jetbrains.com/docs/intellij/plugin-configuration-file.html -->
<idea-plugin>
    <id>cn.com.toyota.plugins.template</id>
    <name>DmsToolsPlugins</name>
    <vendor email="wenguoxing@126.com" url="https://github.com/wenguoxing">温国兴</vendor>

    <description><![CDATA[
        This plugin is study for Dms.<br>
        Support Ap-java,Ap-jsp,Ap-properties,Batch-java,ApBatch-java etc.<br>
        <a href="https://github.com/wenguoxing">source code and document</a><br>
        <p>
            能帮助Dms开发者快速开发
        </p>
        ]]>
    </description>

    <change-notes><![CDATA[
        <h3>v1.3.0 update</h3>
        <ul>
            <li>add @link label in @return doc of the method javadoc</li>
            <li>添加一些功能.</li>
        </ul>
        <h3>v1.2.0 first version</h3>
        <ul>
            <li>初始提交.</li>
        </ul>
        <a href="https://github.com/wenguoxing">source code and document</a><br>
        ]]>
    </change-notes>

<!--    <depends>com.intellij.modules.platform</depends>-->
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <applicationService serviceImplementation="cn.bugstack.guide.idea.plugin.config.JavaDocConfigComponent" order="first"/>
        <applicationService serviceImplementation="cn.bugstack.guide.idea.plugin.service.DocGeneratorService"/>
        <applicationService serviceImplementation="cn.bugstack.guide.idea.plugin.service.WriterService"/>
        <applicationService serviceImplementation="cn.bugstack.guide.idea.plugin.service.VariableGeneratorService"/>
        <applicationConfigurable displayName="DmsJavaDoc" instance="cn.bugstack.guide.idea.plugin.ui.CommonConfigurable">
            <configurable instance="cn.bugstack.guide.idea.plugin.ui.template.ClassConfigurable"/>
            <configurable instance="cn.bugstack.guide.idea.plugin.ui.template.MethodConfigurable"/>
            <configurable instance="cn.bugstack.guide.idea.plugin.ui.template.FieldConfigurable"/>
        </applicationConfigurable>
    </extensions>

<!--    <applicationListeners>-->
<!--        <listener class="org.jetbrains.plugins.template.listeners.MyProjectManagerListener"-->
<!--                  topic="com.intellij.openapi.project.ProjectManagerListener"/>-->
<!--    </applicationListeners>-->

    <actions>

        <action id="DmsCpTool"
                class="cn.bugstack.guide.idea.plugin.action.DmsCpToolAction"
                text="Ap-Copy"
                description="AP拷贝">
        </action>
        <action id="DmsCpToolWeb"
                class="cn.bugstack.guide.idea.plugin.action.DmsCpToolWebAction"
                text="Web-Copy"
                description="Web拷贝">
        </action>
        <action id="DmsCpToolBatch"
                class="cn.bugstack.guide.idea.plugin.action.DmsCpToolBatchAction"
                text="Batch-Copy"
                description="Batch拷贝">
        </action>
        <action id="DmsCpToolApBatch"
                class="cn.bugstack.guide.idea.plugin.action.DmsCpToolAPBatchAction"
                text="APBatch-Copy"
                description="APBatch拷贝">
        </action>
        <action id="DmsCpToolResourceBatch"
                class="cn.bugstack.guide.idea.plugin.action.DmsCpToolResourceAction"
                text="Resource-Copy"
                description="AP资源拷贝">
        </action>

        <action id="JavaDocGenerator" class="cn.bugstack.guide.idea.plugin.action.JavadocGenerateAction" text="JavaDocGenerate"
                description="JavaDocGenerate">
            <keyboard-shortcut keymap="Mac OS X" first-keystroke="meta BACK_SLASH"/>
            <keyboard-shortcut keymap="Mac OS X 10.5+" first-keystroke="meta BACK_SLASH"/>
            <keyboard-shortcut keymap="Default for XWin" first-keystroke="ctrl BACK_SLASH"/>
            <keyboard-shortcut keymap="$default" first-keystroke="meta BACK_SLASH"/>
            <keyboard-shortcut keymap="$default" first-keystroke="ctrl BACK_SLASH"/>
        </action>

        <!-- Add your actions here -->
        <!--            <add-to-group group-id="MainMenu" anchor="after" relative-to-action="com.jlh.mybatis.view.MVPSupportAction" />-->

        <group id="PubEditorPluginDms.Menu" text="_DmsTools" description="DmsTool plugin menu">
            <add-to-group group-id="MainMenu" anchor="last"/>
            <reference ref="DmsCpTool"/>
            <reference ref="DmsCpToolWeb"/>
            <reference ref="DmsCpToolBatch"/>
            <reference ref="DmsCpToolApBatch"/>
            <reference ref="DmsCpToolResourceBatch"/>
            <separator text="*  JavaDocGenerator"/>
            <group id="PubEditorPluginDms.Menu.Other" popup="true" text="Other">
                <reference ref="JavaDocGenerator"/>
            </group>
        </group>

        <group id="dmstool-plugin" popup="true" text="DmsTools">
            <add-to-group group-id="EditorPopupMenu" anchor="before" relative-to-action="$Paste"/>
            <!--            右键到文件夹区的处理-->
            <add-to-group group-id="CutCopyPasteGroup" anchor="before" relative-to-action="$Paste"/>
            <separator/>
            <reference ref="DmsCpTool"/>
            <reference ref="DmsCpToolWeb"/>
            <reference ref="DmsCpToolBatch"/>
            <reference ref="DmsCpToolApBatch"/>
            <reference ref="DmsCpToolResourceBatch"/>
            <separator/>
            <group id="dmstool-plugin-other" popup="true" text="Other">
                <reference ref="JavaDocGenerator"/>
            </group>
        </group>

